    "version": "0.2.0",
    "configurations": [
        {
            // J-Link Debugging of STM32H537 using Cortex-Debug - Assumes J-Link Directory on PATH
            "name": "Cortex Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${command:cmake.launchTargetPath}",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "device": "STM32H573VITx"
        }
    ]
}